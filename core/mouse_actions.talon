click:
    mouse_click(0)
    user.grid_close()
    user.mouse_drag_end()

tap:
    mouse_click(0)
    user.grid_close()
    user.mouse_drag_end()

right click:
    mouse_click(1)
    user.grid_close()

middle click:
	mouse_click(2)
	# close the mouse grid
	user.grid_close()

open in new tab: user.control_click()
control click: user.control_click()


